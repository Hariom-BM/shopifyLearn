
{% if page.handle == 'engagement-stories' -%}
	{% section 'engagement-stories' %}	
  
  {% else %}
  {% include 'breadcrumbs' %}
  <div class="wrapper page-wrapper">
      <h1>{{ page.title }}</h1>
    
      <div class="rte">
        <div class="sitemap-collections-list">
        <h2>Collection</h2>
        <ul>
          {% for collection in collections %}
        <li>
          <a href="{{collection.url}}">{{collection.title}}</a>
          
          </li>
          {% endfor %}
        </ul>
        </div>
        <div class="sitemap-pages-list">
        <h2>Pages</h2>
        <ul>
        {% for link in linklists.footer.links %}
        
          <li>
            <a href="{{link.url}}">{{link.title}}</a>
          </li>
          {% endfor %}
        </ul>
              
      </div>
  </div>

{% endif %}
